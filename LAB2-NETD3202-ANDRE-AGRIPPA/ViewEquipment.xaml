<!--Beginning of user control, variable names found here-->
    <UserControl x:Class="LAB2_NETD3202_ANDRE_AGRIPPA.ViewEquipment"
                 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                 xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                 xmlns:local="clr-namespace:LAB2_NETD3202_ANDRE_AGRIPPA"
                 mc:Ignorable="d" 
                 d:DesignHeight="450" d:DesignWidth="800">
        <!--Beginning of Grid, this is where I place anything going on the user control-->
        <Grid>
            <!--Beginning of stack panel-->
            <StackPanel Grid.Column="1" Margin="10">
                <Label x:Name="txtViewEquipment" Content="View Equipment Lent Out" HorizontalAlignment="Center"/>
                <!--Beginning of data grid-->
                <DataGrid Name="viewEquipmentGrid"/>
             </StackPanel>
            <!--End of stack panel-->
        </Grid>
        <!--End of grid-->
</UserControl>
<!-- End of user control-->
